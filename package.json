{
  "name": "vue-sfc-temp",
  "version": "1.0.0",
  "description": "Vue 组件库--脚手架模板",
  "author": "Sport",
  "license": "MIT",
  "keywords": [
    "vue",
    "sfc",
    "component"
  ],
  "files": [
    "src",
    "dist",
    "yarn.lock"
  ],
  "main": "dist/tempUI.umd.js",
  "module": "dist/tempUI.esm.js",
  "unpkg": "dist/tempUI.min.js",
  "private": false,
  "scripts": {
    "story": "start-storybook -p 9001 -c .storybook",
    "story:build": "build-storybook -c .storybook -o docs/storybook",
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:umd": "rollup -c rollup.config.js --format umd --file dist/tempUI.umd.js",
    "build:es": "rollup -c rollup.config.js --format es --file dist/tempUI.esm.js",
    "build:unpkg": "rollup -c rollup.config.js --format iife --file dist/tempUI.min.js"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/vue": "^5.0.6",
    "autoprefixer": "^9.5.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "iview": "^3.3.3",
    "postcss": "^7.0.14",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10"
  }
}
